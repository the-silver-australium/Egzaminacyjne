<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Margin="10">
            <Label FontSize="20" FontWeight="DemiBold">Wzory aplikacji desktopowych</Label>
        <Button Click="Button_Click" Margin="20"  >Styczeń 2024</Button>
        <Button Margin="20">Styczeń 2023</Button>
        <Button Margin="20">Czerwiec 2023</Button>
        <Button Margin="20">Egzamin</Button>
        </StackPanel>
    </Grid>
</Window>
